{
	"name": "wait-for-vercel-preview",
	"version": "1.3.3",
	"description": "A GitHub Action that waits for Vercel preview deployments to be ready",
	"type": "module",
	"main": "dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/patrickedqvist/wait-for-vercel-preview.git"
	},
	"scripts": {
		"test": "vitest",
		"test:ci": "vitest run",
		"test:ci:coverage": "vitest run --coverage",
		"build": "ncc build index.js -o dist",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write ."
	},
	"keywords": [
		"github-action",
		"vercel",
		"preview",
		"deployment"
	],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/patrickedqvist/wait-for-vercel-preview/issues"
	},
	"homepage": "https://github.com/patrickedqvist/wait-for-vercel-preview#readme",
	"dependencies": {
		"@actions/core": "^1.11.1",
		"@actions/github": "^6.0.0",
		"@vercel/ncc": "^0.38.4",
		"axios": "^1.13.2",
		"set-cookie-parser": "^2.7.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@vitest/coverage-v8": "^3.1.4",
		"msw": "^2.12.7",
		"vitest": "^3.1.4"
	}
}
